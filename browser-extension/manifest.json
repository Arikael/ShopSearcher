{
  "name": "ShopSearcher",
  "version": "2.1.0",
  "manifest_version": 2,
  "description": "Opens a list of shop URLs which are parameterized",
  "homepage_url": "https://github.com/Arikael/ShopSearcher",
  "browser_action": {
    "default_title": "Search multiple shops at once",
    "default_popup": "popup/index.html",
    "theme_icons": [{
      "light": "icons/icon-light-32.png",
      "dark": "icons/icon-dark-32.png",
      "size": 32
    },
      {
        "light": "icons/icon-light-16.png",
        "dark": "icons/icon-dark-16.png",
        "size": 16
      }]
  },
  "permissions": [],
  "incognito": "spanning",
  "browser_specific_settings": {
    "gecko": {
      "id": "{a86e4025-43ca-4e5f-aecd-a739ef38d5a7}",
      "strict_min_version": "57.0"
    }
  }
}
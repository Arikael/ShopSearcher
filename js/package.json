{
  "name": "shop-searcher",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:firefox": "npm run build && shx rm -rf ./browser-extension/popup/* && shx cp -r ./dist/* ./browser-extension/popup",
    "run:firefox": "npm run build:firefox && web-ext run --source-dir ./browser-extension",
    "run:firefox-mobile": "npm run build:firefox && cd browser-extension && web-ext run -t firefox-android --",
    "preview": "vite preview",
    "check": "svelte-check --tsconfig tsconfig.json"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^2.0.0",
    "@tsconfig/svelte": "^3.0.0",
    "svelte": "^3.52.0",
    "svelte-check": "^2.9.2",
    "svelte-preprocess": "^5.0.0",
    "tslib": "^2.4.0",
    "typescript": "^4.6.4",
    "vite": "^4.0.0",
    "web-ext": "^7.4.0",
    "shx": "^0.3.4"
  }
}
{
  "name": "shop-searcher",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build --outDir ./dist/svelte",
    "build:firefox": "npm run build:firefox && web-ext build --source-dir ./browser-extension --artifacts-dir ./dist/firefox --overwrite-dest",
    "run:firefox": "node scripts/sync-for-firefox.js && node scripts/run-firefox.js --browser firefox",
    "run:firefox-mobile": "node scripts/sync-for-firefox.js --browser firefox-mobile",
    "preview": "vite preview",
    "check": "svelte-check --tsconfig tsconfig.json"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^2.0.0",
    "@tsconfig/svelte": "^3.0.0",
    "dotenv": "^16.0.3",
    "svelte": "^3.52.0",
    "svelte-check": "^3.0.0",
    "svelte-preprocess": "^5.0.0",
    "tslib": "^2.4.0",
    "typescript": "^4.6.4",
    "vite": "^4.0.0",
    "web-ext": "^7.4.0"
  }
}
